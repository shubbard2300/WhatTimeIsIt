{
  "crons": [
    {
      "path": "/api/process-scheduled-calls",
      "schedule": "* * * * *"
    }
  ],
  "bypassProtectionForPaths": [
    "/api/twilio/*",
    "/audio/*"
  ],
  "headers": [
    {
      "source": "/api/twilio/(.*)",
      "headers": [
        {
          "key": "X-Robots-Tag",
          "value": "noindex"
        }
      ]
    },
    {
      "source": "/audio/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/api/twilio/:path*",
      "destination": "/api/twilio/:path*"
    }
  ]
}
